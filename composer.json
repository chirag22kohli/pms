{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.1.3",
        "appzcoder/laravel-admin": "^2.1",
        "bacon/bacon-qr-code": "1.0.*",
        "codecasts/laravel-jwt": "^0.10.0",
        "fideloper/proxy": "^4.0",
        "folklore/image": "^0.3.22",
        "intervention/image": "^2.3",
        "laravel/framework": "5.6.*",
        "laravel/passport": "^7.0",
        "laravel/tinker": "^1.0",
        "maatwebsite/excel": "~2.1.0",
        "pawlox/video-thumbnail": "^1.0",
        "paypal/rest-api-sdk-php": "^1.13",
        "php-junior/laravel-html-encrypt": "^1.3",
        "rap2hpoutre/laravel-stripe-connect": "^0.2.1",
        "rbdwllr/reallysimplejwt": "^2.0",
        "simplesoftwareio/simple-qrcode": "~2",
        "softon/sweetalert": "^1.0"
    },
    "require-dev": {
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
